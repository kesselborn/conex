{
  "manifest_version": 2,
  "name": "Taborama",
  "version": "0.1.1",
  "applications": {
    "gecko": {
      "id": "{ec9d70ea-0229-49c0-bbf7-0df9bbccde35}"
    }
  },
  "browser_action": {
    "default_title": "Taborama",
    "default_popup": "browser-action.html",
    "default_icon": {
      "48": "icons/icon_48.png"
    }
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "MacCtrl+Space"
      }
    },
    "_execute_page_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y"
      }
    },
    "new-tab-in-same-group": {
      "suggested_key": {
        "default": "Ctrl+Y"
      }
    }
  },
  "options_ui": {
    "page": "options_ui.html"
  },
  "page_action": {
    "browser_style": true,
    "default_popup": "page-action.html",
    "default_icon": {
      "19": "icons/icon_19.png",
      "38": "icons/icon_38.png"
    },
    "default_title": "re-open to tab in other group (CTRL+Shift+Y)"
  },
  "background": {
    "scripts": [
      "taborama.js"
    ]
  },
  "icons": {
    "48": "icons/icon_48.png"
  },
  "permissions": [
    "<all_urls>",
    "contextualIdentities",
    "cookies",
    "history",
    "storage",
    "tabs",
    "unlimitedStorage",
    "webNavigation"
  ]
}
